      <script type="lazyload_int">
        function convertDataAttributes() {
            const videos = document.querySelectorAll('video.myVideo');
            videos.forEach(video => {
              if (video.hasAttribute('data-src')) {
                const dataSrc = video.getAttribute('data-src');
                video.setAttribute('src', dataSrc);
              }
              if (video.hasAttribute('data-poster')) {
                const dataPoster = video.getAttribute('data-poster');
                video.setAttribute('poster', dataPoster);
              }
            });
          }
          convertDataAttributes();
      </script>
      <script>
          var site_nav_link_burger = true;
          var site_search = true;
          var site_cart = true;
          document.addEventListener("DOMContentLoaded", function() {
              var icon_menu = document.querySelector("summary.header__icon.header__icon--menu");
              var icon_menu_1 = document.querySelector("summary.header__icon.header__icon--search");
              var icon_menu_2 = document.querySelector("a#cart-icon-bubble");
              icon_menu.addEventListener("click", function() {
                  if(site_nav_link_burger) {
                      event.preventDefault();
                      site_nav_link_burger = false;
                  }
              });
              icon_menu_1.addEventListener("click", function() {
                  if(site_search) {
                      event.preventDefault();
                      site_search = false;
                  }
              });
              icon_menu_2.addEventListener("click", function() {
                  if(site_cart) {
                      event.preventDefault();
                      site_cart = false;
                  }
              });
          })
      </script>
